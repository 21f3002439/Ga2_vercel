{
    "version": "python3.9",
    "builds": [
        {
            "src": "vercel.json",
            "use": "vercel-sapper"
        },
        {
            "src": "api/index.js",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}